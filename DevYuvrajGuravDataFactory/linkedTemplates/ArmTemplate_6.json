		{
			"name": "[concat(parameters('factoryName'), '/df013Lookup')]",
			"type": "Microsoft.DataFactory/factories/dataflows",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "MappingDataFlow",
				"typeProperties": {
					"sources": [
						{
							"dataset": {
								"referenceName": "DS_SALES",
								"type": "DatasetReference"
							},
							"name": "ReadSales"
						},
						{
							"dataset": {
