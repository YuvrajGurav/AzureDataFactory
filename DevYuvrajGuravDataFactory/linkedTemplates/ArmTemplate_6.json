								"type": "DatasetReference"
							},
							"name": "ReadProducts"
						},
						{
							"dataset": {
								"referenceName": "DS_CUSTOMERS",
								"type": "DatasetReference"
							},
							"name": "ReadCustomers"
						}
					],
					"sinks": [
						{
							"dataset": {
								"referenceName": "DS_SQL_LOOKUP",
								"type": "DatasetReference"
