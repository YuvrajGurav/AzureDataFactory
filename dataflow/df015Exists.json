{
	"name": "df015Exists",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_SALES",
						"type": "DatasetReference"
					},
					"name": "ReadSales"
				},
				{
					"dataset": {
						"referenceName": "DS_PRODUCTS",
						"type": "DatasetReference"
					},
					"name": "ReadProducts"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DS_SQL_EXISTS",
						"type": "DatasetReference"
					},
					"name": "sink"
				}
			],
			"transformations": [
				{
					"name": "exists"
				}
			],
			"scriptLines": [
				"source(output(",
				"          ProductKey as short,",
				"          CustomerKey as short,",
				"          SalesAmount as double",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> ReadSales",
				"source(output(",
				"          ProductKey as short,",
				"          EnglishProductName as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> ReadProducts",
				"ReadSales, ReadProducts exists(ReadSales@ProductKey == ReadProducts@ProductKey,",
				"     negate:false,",
				"     broadcast: 'auto')~> exists",
				"exists sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> sink"
			]
		}
	}
}